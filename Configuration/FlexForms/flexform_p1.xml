<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Function</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<switchableControllerActions>
						<TCEforms>
							<label>Select function</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">Agenda expositions</numIndex>
										<numIndex index="1">Exposition->list;Exposition->show</numIndex>
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">Galerie</numIndex>
										<numIndex index="1">Oeuvres->list;Oeuvres->show</numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">Mise en valeur</numIndex>
										<numIndex index="1">Oeuvres->highlight</numIndex>
									</numIndex>
									<numIndex index="3">
										<numIndex index="0">Liste des artistes</numIndex>
										<numIndex index="1">Artiste->list;Artiste->show;Artiste->search</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>
					<settings.detailPid>
						<TCEforms>
							<label>LLL:EXT:news/Resources/Private/Language/locallang_be.xlf:flexforms_additional.detailPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.detailPid>
					<settings.limitHighlight>
						<TCEforms>
							<label>Limit highlight</label>
							<config>
								<type>input</type>
								<size>5</size>
								<eval>num</eval>
								<default>3</default>
							</config>
						</TCEforms>
					</settings.limitHighlight>
					<settings.limitPagination>
						<TCEforms>
							<label>Limit Pagination</label>
							<config>
								<type>input</type>
								<size>5</size>
								<eval>num</eval>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.limitPagination>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
